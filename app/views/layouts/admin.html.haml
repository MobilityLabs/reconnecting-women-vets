!!!
= render partial: 'layouts/common'
= render partial: 'layouts/admin_session'
%html
  %head
    = content_for :title
    = stylesheet_link_tag    "application"
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body
    #wrap
      #main
        .container
          .row-fluid
            .span12
              = render partial: 'layouts/masthead'
          .row-fluid
            .span12
              = render partial: 'layouts/notice_and_alert'
              = yield
        #push
    = content_for :footer
